
{
  "manifest_version": 3,
  "name": "THED IVU Bot Executor",
  "version": "1.0",
  "permissions": ["scripting", "tabs", "activeTab"],
  "host_permissions": ["http://localhost:8000/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
